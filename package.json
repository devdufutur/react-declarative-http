{
  "name": "react-declarative-http",
  "version": "0.0.11",
  "type": "module",
  "description": "Lightweight React library enabling (really) simple data fetching by combining the better of imperative & declarative paradigms",
  "browserslist": [
    "Firefox 58",
    "Chrome 66"
  ],
  "files": [
    "dist/*"
  ],
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "buncheeBuild": "bunchee ./src/index.js",
    "build": "npm run build:esm",
    "test": "vitest",
    "build:cjs": "babel ./src -x .js,.jsx --out-dir ./dist/cjs --env-name cjs --delete-dir-on-start",
    "build:esm": "babel ./src -x .js,.jsx --out-dir ./dist/esm --env-name esm --delete-dir-on-start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devdufutur/react-declarative-http.git"
  },
  "keywords": [
    "react",
    "javascript",
    "fetch",
    "api",
    "http",
    "library",
    "declarative",
    "imperative"
  ],
  "author": "DevDuFutur (Rudy NappÃ©e)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/devdufutur/react-declarative-http/issues"
  },
  "homepage": "https://github.com/devdufutur/react-declarative-http#readme",
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.8",
    "@babel/eslint-parser": "7.17.0",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@vitejs/plugin-react": "^2.1.0",
    "bunchee": "^2.1.2",
    "jsdom": "^20.0.0",
    "msw": "^0.47.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^3.1.0",
    "vitest": "^0.23.2"
  },
  "peerDependencies": {
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
  },
  "dependencies": {
    "prop-types": "^15.8.1",
    "whatwg-fetch": "^3.6.2"
  }
}
